{
    "name": "@apus.run/utils",
    "type": "module",
    "version": "0.1.0",
    "packageManager": "pnpm@8.6.0",
    "description": "JavaScript & TypeScript常用工具集",
    "author": "moocss <moocss@gmail.com>",
    "license": "MIT",
    "funding": "https://github.com/sponsors/moocss",
    "homepage": "https://github.com/apus-run/apus.run#readme",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/apus-run/apus.run.git"
    },
    "bugs": "https://github.com/apus-run/apus.run/issues",
    "keywords": [],
    "sideEffects": false,
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "exports": {
      ".": "./src/index.ts"
    },
    "publishConfig": {
      "access": "public",
      "module": "./dist/index.mjs",
      "main": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "exports": {
        ".": {
          "types": "./dist/index.d.ts",
          "import": "./dist/index.mjs",
          "require": "./dist/index.js"
        },
        "./*": "./*"
      },
      "files": [
        "dist"
      ]
    },
    "scripts": {
      "build": "vite build",
      "dev": "vite build --watch",
      "prepublishOnly": "pnpm build",
      "release": "bumpp && npm publish",
      "start": "esno src/index.ts",
      "test": "vitest",
      "typecheck": "tsc --noEmit"
    },
    "simple-git-hooks": {
      "pre-commit": "pnpm lint-staged"
    },
    "lint-staged": {
      "*": "eslint --fix"
    }
  }
  